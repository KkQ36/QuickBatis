<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kq.mybatis.dao.UserDao">

    <select id="queryUserInfoById" parameterType="java.lang.Integer" resultType="com.kq.mybatis.entity.User">
        SELECT *
        FROM person
        where id = #{id}
    </select>

</mapper>
